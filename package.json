{
	"devDependencies": {
		"@babel/core": "^7.3.3",
		"@babel/plugin-proposal-class-properties": "^7.3.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.3.2",
		"@babel/plugin-transform-runtime": "^7.2.0",
		"@babel/preset-env": "^7.3.1",
		"@babel/preset-react": "^7.0.0",
		"@storybook/addon-actions": "^5.0.8",
		"@storybook/addon-info": "^5.0.8",
		"@storybook/react": "^5.0.8",
		"babel-loader": "^8.0.5",
		"enzyme": "^3.8.0",
		"enzyme-adapter-react-16": "^1.9.1",
		"husky": "^1.3.1",
		"jest": "^24.1.0",
		"jest-styled-components": "^6.2.1",
		"lerna": "^3.13.2",
		"lint-staged": "^8.1.3",
		"prettier": "^1.16.4",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-draggable-playground": "^1.0.0",
		"react-testing-library": "^5.2.3",
		"rollup": "^1.11.3",
		"rollup-plugin-babel": "^4.3.2",
		"rollup-plugin-babel-runtime-external": "^2.0.0",
		"rollup-plugin-commonjs": "^9.2.0",
		"rollup-plugin-filesize": "^6.0.1",
		"rollup-plugin-json": "^3.1.0",
		"rollup-plugin-node-resolve": "^4.0.0",
		"styled-components": "^3.4.10"
	},
	"jest": {
		"setupFilesAfterEnv": [
			"./test-setup.js"
		],
		"coverageReporters": [
			"lcov",
			"html"
		],
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"dist"
		],
		"coverageThreshold": {
			"global": {
				"branches": 90,
				"functions": 90,
				"lines": 90,
				"statements": 90
			}
		},
		"testMatch": [
			"<rootDir>/packages/**/test/**/*.js"
		]
	},
	"lint-staged": {
		"*.{js,md}": [
			"prettier --no-semi --single-quote --write",
			"git add"
		]
	},
	"keywords": [
		"design",
		"system",
		"react",
		"styled-system",
		"styled-components"
	],
	"author": "justin.michaliga@gmail.com",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jmichaliga/masonry/issues"
	},
	"homepage": "https://github.com/jmichaliga/masonry#readme",
	"dependencies": {
		"@reach/component-component": "^0.1.1",
		"camelcase": "^5.0.0",
		"npm": "^6.9.0",
		"polished": "^3.2.0",
		"prop-types": "^15.7.2",
		"react-map-gl": "^4.1.2",
		"react-test-renderer": "^16.8.6",
		"styled-system": "^3.2.1",
		"stylis": "^3.4.9"
	},
	"name": "masonry"
}
